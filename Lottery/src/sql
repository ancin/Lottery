

CREATE TABLE `prize` (
	`ID` VARCHAR(45) NOT NULL COLLATE 'latin1_swedish_ci',
	`PRIZE_NAME` VARCHAR(30) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`PRIZE_LEVEL` VARCHAR(5) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`TOTAL_NUM` INT(11) NULL DEFAULT NULL,
	`LEFT_NUM` INT(11) NULL DEFAULT NULL,
	`PICTURE` BLOB NULL,
	`DESCRIPTION` VARCHAR(100) NULL DEFAULT NULL COLLATE 'utf8_bin',
	`GMT_CREATE` TIMESTAMP NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`GMT_MODIFIED` TIMESTAMP  NULL DEFAULT '0000-00-00 00:00:00',
	PRIMARY KEY (`ID`)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;